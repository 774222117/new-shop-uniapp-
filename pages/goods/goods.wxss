/* 海报 */
.posterModal {
  display: none;

  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  background: rgba(0,0,0,.6);
  bottom: 0;
  left: 0;
  opacity: 0;
  outline: 0;
  -webkit-perspective: 1141px;
  perspective: 1141px;
  position: fixed;
  right: 0;
  text-align: center;
  top: 0;
  z-index: 1110;
}
.posterModalShow{
  display: flex;
  align-items: center;
  opacity: 1;
}
.posterBox {
  width: 100%;
  display: inline-block;
  vertical-align: middle;
}

.closeBox {
  width: 658rpx;
  height: 46rpx;
