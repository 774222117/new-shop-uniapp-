/* pages/shop/shop.wxss */
page{
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: #fff
}
/* 外层大盒子 */
.shopMainBox{
  width: 100%;
  height: 100%;
} 
/* 上面的地址栏 */
.topBox{
  width: 100%;
  height: 64rpx;
  background-color: #fff;
  align-items: center;
  font-weight: bold;
}
/* 下面商品列表盒子 */
.shopListBox{
  margin-bottom: 100rpx;
  margin-top: 12rpx;
}
.leftListSty{
  display: flex !important;
  justify-content: center;
  align-items: center;
}
.leftListSty .leftListIcon{
  width: 32rpx;
  height: 32rpx;
  /* margin-right: 10rpx; */
}
.leftListSty .leftListIcon image{
  width: 100%;
  height: 100%;
  display: block;
}
.VerticalNav.nav {
  width: 160rpx;
  background:rgba(242,242,242,1);
}

.VerticalNav.nav .cu-item {
  width: 160rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  background:rgba(242,242,242,1);
  margin: 0;
  border: none;
  position: relative;
  font-size:26rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:rgba(32,32,32,1);  
}

.VerticalNav.nav .cu-item.cur {
  background-color: #fff;
  font-size:26rpx;
  font-family:PingFangSC-Semibold,PingFang SC;
  font-weight:600;
  color:#FF3B30;
  line-height:36rpx;
}
.curheadItem{
  color: #FF3B30!important;
  font-weight: 600;
}
.VerticalBox{
  display: flex;
}
.VerticalMain{
  background-color: #fff;
}

