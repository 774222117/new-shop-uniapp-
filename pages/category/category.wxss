/* 商品列表item的样式 */
.shopItemList{
  /* height: 224rpx; */
  width: 590rpx;
  background: white;
  padding: 22rpx 30rpx 20rpx 20rpx;
  display: flex;
    align-items: center;
}
.shopItemList .leftImg{
  width: 180rpx;
  height: 180rpx;
  position: relative;
}
.shopItemList .leftImg .mainImg{
  width: 160rpx;
  height: 160rpx;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

.shopItemList .leftImg .mainImg image{width: 100%;height: 100%;display: block}
.shopItemList .leftImg .leftTopImg{
  width: 80rpx;
  height: 32rpx;
  position: absolute;
  top: 0;
  left: 0;
  font-size:11px;
  color:rgba(255,255,255,1);
  text-align: center;
  border-radius: 6rpx
}
.leftTopImg image{
  width:80rpx;
  height:32rpx;
}

.shopItemList .rightBox{
  width: 344rpx;
  /* height: 180rpx; */
}
.shopItemList .rightBox .topTitle{
  width: 100%;
  height: 72rpx;
  text-align: left;
  font-weight: 600;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all; /* 追加这一行代码 */
  font-size:26rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  color:rgba(32,32,32,1);
  line-height:36rpx;

}
.shopItemList .rightBox .limitBox{
  width: 100%;
  height: 36rpx;
  font-size:26rpx;
  color:rgba(255,59,48,1);
}
.shopItemList .limitBox1{
  color:rgba(155,155,155,1);
  font-size:26rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  line-height:36rpx;
  }
.shopItemList .rightBox .rightBoxBtmBox{
  width: 100%;
}
.shopItemList .rightBox .rightBoxBtmBox .priceBox{
  width: 100%;
}
.shopItemList .rightBox .text-money1{
  font-size:28rpx;
  font-family:PingFang-SC-Heavy,PingFang-SC;
  font-weight:800;
  color:rgba(255,59,48,1);
  line-height:40rpx;
  margin-right: 10rpx;
  }
.shopItemList .rightBox .text-money2{
  font-size:26rpx;
  font-family:PingFang-SC-Bold,PingFang-SC;
  font-weight:bold;
  color:rgba(155,155,155,1);
  line-height:36rpx;
  text-decoration:line-through;}
.shopItemList .rightBox .countBox{
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
}
.shopItemList .rightBox .countBox .buyBtn{
  width: 142rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  color:rgba(255,59,48,1);
  font-size: 22rpx;
  font-weight: bold;
  border-radius:24rpx;
  border:1px solid rgba(255,59,48,1);
}
 .minusBtn,.plusBtn{
  width: 44rpx;
  height: 44rpx;
  background-color: #FF3B30;
  color: white;
  text-align: center;
  line-height: 44rpx;
  border-radius: 50%;
  font-size: 34rpx;
}
.goosNum{
  width: 84rpx;
  height: 44rpx;
