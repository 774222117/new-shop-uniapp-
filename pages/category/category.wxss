  line-height: 44rpx;
}
.shopCar-image{
  width: 44rpx;
  height: 44rpx;
}

.bottomLoadConpleteCUR{
  width: 448rpx;
  /* height:869rpx; */
  margin: 38rpx auto;    /*220rpx auto */
  font-size:14px;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  background-color: white;
  color:rgba(136,136,136,1);
  height: 56rpx;
  border: 1px solid rgba(32,32,32,0.20000000298023224);
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.headCategory{
  width: 700rpx;
  height: 174rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  position: relative;
  background-color: white;
  z-index: 999;
}
.headCategoryMore {
  width: 100%;
  position: fixed;
  /* top: -334rpx; */
  display: flex;
  flex-wrap: wrap;
  background-color: white;
  /* position: relative; */
  z-index: 120;
  justify-content: space-between;
  border-radius: 0rpx 0rpx 30rpx 30rpx;
  border-top: 2rpx black solid;
}
.cateMore {
  text-align: center;
  position: relative;
  writing-mode: vertical-lr;
  letter-spacing: 20rpx;
  top: -16rpx;
  left: -20rpx;
  font-size: 24rpx;
}
.cateContent {
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: white;
  position: relative;
  z-index: 1111;
}
.allBlack {
	position: fixed;
	top: 0rpx;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 111;
	outline: 0;
	text-align: center;
	-ms-transform: scale(1.185);
	transform: scale(1.185);
	backface-visibility: hidden;
	perspective: 2000rpx;
	background: rgba(0, 0, 0, 0.6);
	transition: all 0.3s ease-in-out 0s;
	pointer-events: none;
}
.cateMoreBtn {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  position: absolute;
  left: 2rpx;
  right: 0;
  bottom: -16rpx;
  margin: 0 auto;
  transition-duration: 0.5s;
}
.cateMoreBtn image {
  width: 100%;
  height: 100%;
  position: absolute;
}
.headItem{
  display: flex;
  flex-direction: column;
