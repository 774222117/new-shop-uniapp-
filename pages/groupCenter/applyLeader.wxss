.apply-tips{
  padding: 28rpx 32rpx 0 32rpx;
}
.cu-form-group{
  justify-content: left!important;
}
.apply-tips text{
  font-size:26rpx;
  font-weight:400;
  line-height:36rpx;
  margin-top: 16rpx;
  color: red;
}
.apply-btn{
  padding: 28rpx 32rpx;
}
.apply-btn button{
  background-color: #FF3B30;
  color: #FFFFFF;
  font-size:32rpx;
  font-weight:400;
  height:92rpx;
  line-height: 92rpx;
}
.apply-main>.cu-form-group>.title{
  width: 170rpx;
}
.apply-main>.cu-form-group>.content{
  color: #555;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.cu-dialog.pickAddrBox{
  border-top-left-radius: 50rpx !important;
  border-top-right-radius: 50rpx !important;
}
.pickAddr{
  display: flex;
  flex-direction: row;
}
.pickAddr>.title{
  width: 180rpx;
}
.pickAddr>.content{
  font-size:26rpx;
  font-family:PingFangSC-Semibold,PingFang SC;
  font-weight:600;
  color:rgba(32,32,32,1);
  line-height:36rpx;
  display: flex;
  align-items: center;
}
.pickAddrInfo{
  display: flex;
  flex-direction: column;
  font-size:28rpx;
  font-family:PingFang-SC-Bold,PingFang-SC;
  font-weight:bold;
  color:rgba(32,32,32,1);
  line-height:40rpx;
}
.pickAddrInfo-desc{
  font-size:24rpx;
  font-family:PingFang-SC-Medium,PingFang-SC;
  font-weight:500;
  color:#B6B6B6;
  line-height:34rpx;
}