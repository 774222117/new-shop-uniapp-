
.videoMain{
  width:100%;
  height: 100vh;
}
.gouwu{
  position: fixed;
  bottom: 80rpx;
  right: 30rpx;
  z-index: 10;
  color: #fff;
  background: #7574EE;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 50rpx;
}
/* 门店选择列表 */
.cu-dialog.videoGoodsBox{
  border-top-left-radius: 25px !important;
  border-top-right-radius: 25px !important;
}
.videoGoodsTitle{
  height: 138rpx;
}
.titleLeft{
  display: flex;
  flex: 1;
  justify-content: center;
}
.goods-card{
  display: flex;
  flex-direction: row; 
  border-radius: 20rpx;
  background-color: white;
  margin: 8rpx 0;
  padding: 16rpx 16rpx;
}
.goods-img{
  width: 120rpx;
  height: 120rpx;
}
.goods-img image{
  width: 120rpx;
  height: 120rpx;
}
.goods-info{
  display: flex;
  flex-direction: column;
  flex: 1;
  padding: 0 20rpx;
  align-items: flex-start;
  justify-content: space-between
}
/*2行超出截断*/
.text-ellipsis{
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all; /* 追加这一行代码 */
}
.text-title{
  font-size: 26rpx;
  line-height: 36rpx;
  height: 72rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(32,32,32,1);
}
.text-price{
  font-size: 26rpx;
  line-height: 36rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgb(212, 145, 86);
}
.bg-videoMain{
  background-color: #F2F2F2;
}