page{
  font-family:PingFangSC-Semibold,PingFang SC;
  height: 100%;
}
.cashout-top{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 28rpx 22rpx;
  background-color: white;
}
.cashout-top text{
  font-size:26rpx;
  font-weight:600;
  color:rgba(32,32,32,1);
  line-height:36rpx;
}
.cashout-top,.realname input{
  text-align: right;
}
.can_cashout{
  padding: 24rpx 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.can_cashout-info{
  font-size:26rpx;
  font-weight:400;
  color:rgba(57,57,57,1);
  line-height:36rpx;
}
.cashout-all{
  color: #FF3B30;
}
.cashout-userinfo{
  background-color: white;
}
.cashout-userinfo view{
  padding: 24rpx 28rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
}
.cashout-userinfo text{
  font-size:26rpx;
  font-weight:600;
  color:rgba(32,32,32,1);
  line-height:36rpx;
  display: flex;
  align-items: center;
}
.cashout-userinfo image{
  width: 32rpx;
  height: 32rpx;
  margin-right: 8rpx;
}
.realname{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.cashout-btn{
  padding: 28rpx 32rpx;
}
.cashout-btn button{
  background-color: #FF3B30;
  color: #FFFFFF;
  font-size:32rpx;
  font-weight:400;
  height:92rpx;
  line-height: 92rpx;
}
.cash-rules{
  padding: 24rpx 0;
  display: flex;
  flex-direction: column;
}
.cash-rules text:first-child{
  font-size:26rpx;
  font-weight:600;
  color:rgba(32,32,32,1);
  line-height:36rpx;
}
.cash-rules text:not(first-child){
  font-size:26rpx;
  font-weight:400;
  color:rgba(32,32,32,1);
  line-height:36rpx;
  margin-top: 16rpx;
}
.cashout-records{
  background-color: white;
  display: flex;
  flex-direction: column;
}
.records-head{
  padding:16rpx 32rpx;
  font-size:26rpx;
  font-weight:600;
  color:rgba(57,57,57,1);
  line-height:36rpx;
  position:relative;
}
.line{
  position: relative;
}
.line::after  {
  content:'';
  position:absolute;  /* 把父视图设置为relative，方便定位*/
  top:0;
  left:0;
  width:200%;
  height:200%;
  transform: scale(0.5);
  transform-origin:0 0;
  box-sizing: border-box;
  border-radius:40rpx;
  border:2rpx solid rgba(0, 0, 0, 0.05);
}
.records-card{
  padding:24rpx 32rpx;

}
.cashout-record{
  display: flex;
  flex-direction: column;
}
.record-top{
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size:32rpx;
  font-weight:500;
  color:rgba(32,32,32,1);
  line-height:44rpx;
  text-align: center;
}
.status-fail{
  font-size:26rpx;
  font-weight:500;
  color:rgba(155,155,155,1);
  line-height:36rpx;
}
.record-date{
  margin-top: 8rpx;
  font-size:26rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(155,155,155,1);
  line-height:36rpx;
}
.cashout_success{
  width: 750rpx;
  display: flex;
  justify-content: center;
  position: absolute;
  top: 382rpx;
  z-index:99;
}
.success-card{
  width:382rpx;
  height:320rpx;
  background:rgba(255,255,255,1);
  box-shadow:0 8rpx 24rpx 0px rgba(0,0,0,0.08);
  border-radius:24rpx;
  display: flex;
  padding-top: 50rpx;
  flex-direction: column;
  align-items: center;
}
.success-card image{
  width: 144rpx;
  height: 144rpx;
}
.success-card text{
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(32,32,32,1);
  line-height:44rpx;
  margin-top: 30rpx;
}
