@import '../../component/wxParse/wxParse.wxss';
/* 轮播 */
.shopadvanceBox{
  width: 100%;
  height: 610rpx;
  background-color: white;
  /* background-color: #F2F2F2; */
  position: relative;
}
.shopadvanceBox .swiperBox{
  width: 648rpx;
  height: 582rpx;
  /* background-color: goldenrod; */
  margin: 0 auto;
}
.shopadvanceBox .swiperBox image{width: 100%;height: 100%;display: block}
/* 限时预售价格 */
.limitedTimeAdvanceSale{
  width: 100%;
  height: 122rpx;
  overflow: none;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background:linear-gradient(101deg,rgba(241,61,41,1) 0%,rgba(250,106,66,1) 66%,rgba(255,146,125,1) 67%,rgba(251,118,83,1) 78%,rgba(250,106,66,1) 100%,rgba(250,106,66,1) 100%);
}
.limitedTimeAdvanceSale .leftAdvanceSale{
  width: 480rpx;
  height: 100%;
  /* background-color: red; */
  font-size:14px;
  color:rgba(255,255,255,1);
  display: flex;
  padding-left: 30rpx;
  flex-direction: column;
  justify-content: center;
  position: relative;
}
.limitedTimeAdvanceSale .leftAdvanceSaleTop {
  font-size: 40rpx;
  display: flex;
  align-items: center;
  position: relative;
}
.limitedTimeAdvanceSale .leftAdvanceSaleTop .lishen{
  width: 130rpx;
  height: 48rpx;
  text-align: center;
  line-height: 48rpx;
  font-size:22rpx;
  color:rgba(200,41,1,1);
  font-weight: bold;
  position: absolute;
  left: 110rpx;
  top: -44rpx;
}
.lishen text{
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 3;
}
.lishen .imgBlock{
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.imgBlock{
  width: 100%;
  height: 100%;
  display: block;
}
.limitedTimeAdvanceSale .leftAdvanceSaleBtm {
  font-size: 22rpx;
  color: rgba(255,255,255,0.8)
}
.limitedTimeAdvanceSale .rightLimitedTime{
  width: 272rpx;
  height: 100%;
  /* background-color: #FCAD22; */
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size:12px;
  font-family:PingFang-SC-Bold,PingFang-SC;
  color:rgba(255,255,255,1);
  padding-left: 30rpx;
}
.limitedTimeAdvanceSale .rightLimitedTime .countDownTime{
  margin-top: 10rpx;
}
.limitedTimeAdvanceSale .rightLimitedTime .countDownTime text{
  width:46rpx;
  height:46rpx;
  font-size:12px;
  font-family:PingFang-SC-Bold,PingFang-SC;
  color:rgba(255,255,255,1);
  line-height:17px;
  margin-right: 14rpx;
  padding: 4rpx;
  background:rgba(220,62,46,1);
  /* background-color: #EF920D; */
}
/* 标题 */
.shopadvanceTitle{
  width: 100%;
  height: 144rpx;
  background: white;
  padding: 20rpx 30rpx 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: relative;
}
.shopadvanceTitle .text1{
  width: 520rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size:32rpx;
  font-weight:bold;
  color:rgba(43,43,43,1);
  margin-bottom: 10rpx;
}
.shopadvanceTitle .text2{
  font-size:26rpx;
  color:rgba(32,32,32,0.4);
  line-height:18px;
}
.shopadvanceTitle .share{
  width:168rpx;
  height:62rpx;
  background:rgba(255,188,188,0.41);
  border-radius:84rpx 0px 0px 84rpx;
  position: absolute;
  right: 0;
  top: 40rpx;
  
}
.shareIcon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 6rpx;
}
.shopadvanceTitle .share .shareButton{
  width: 100%;
  height: 100%;
  background: transparent;
  outline: none;
  border: none;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:24rpx;
  font-weight: bold;
  color:rgba(241,61,41,1);
}
.shareButton::after{
  border: none;
}
/* 提货门店信息 */
.shopadvanceInfo{
  width:100%;
  /* height:212rpx; */
  background:rgba(255,255,255,1);
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.shopadvanceInfo>view{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  padding-left: 30rpx;
  font-size:24rpx;
  font-weight:500;
  color:rgba(155,155,155,1);
}
.shopadvanceInfo>view>text{
  font-size:12px;
  font-weight:400;
  color:rgba(57,57,57,1);
}
/* 商品详情 */
.commodityDetails{
  width: 100%;
  margin-bottom: 100rpx;
}
.commodityDetails .commodityDetailsTitle{
