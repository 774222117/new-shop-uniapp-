  height: 62rpx;
  line-height: 62rpx;
  text-align: center;
  font-size:12px;
  color:rgba(57,57,57,1);
  background-color: white;
}
/* 底部导航 */
.shopadvancebtmBar{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  background-color: white;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 40rpx;
}
.leftShare{
  display: flex;
